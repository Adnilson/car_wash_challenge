<div class="center">

  <%= form_with(scope: :subscription, url: car_subscriptions_path) do |form| %>

    <div class="field">
      <%= form.label :start_date %>
      <%= form.date_field :start_date %>
    </div>

    <div class="field">
      <%= form.label :end_date %>
      <%= form.date_field :end_date %>
    </div>

    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
</div>
